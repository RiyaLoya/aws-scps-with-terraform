{
  "Version": "2012-10-17",
  "Statement": [
     {
      "Sid": "",
      "Effect": "Deny",
      "Action": [
        "ec2:RunInstances"
      ],
      "Resource": [
         "arn:aws:ec2:*:*:instance/*"
       ],
       "Condition": {
        "ForAllValues:StringNotLike": {
          "ec2:InstanceType": [
            "c5.large"
          ]
        }
      }
    }
   ]
}